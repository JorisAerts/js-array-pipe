    "test:coverage": "vitest --typecheck --coverage"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^2.0.4",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.8.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.3.5",
    "vite-plugin-dts": "^4.0.0-beta.1",
    "vitest": "^2.0.4"
  },
  "packageManager": "yarn@4.3.1"
