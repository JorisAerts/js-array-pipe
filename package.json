{
  "name": "array-pipe",
  "version": "0.2.2",
  "description": "JavaScript arrays in a streaming fashion",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JorisAerts/js-array-pipe.git"
  },
  "author": {
    "name": "Joris Aerts",
    "url": "https://github.com/JorisAerts"
  },
  "keywords": [
    "javascript",
    "typescript",
    "array",
    "stream",
    "pipe",
    "pipeline",
    "chaining"
  ],
  "files": [
    "dist/*"
  ],
  "type": "module",
  "main": "dist/array-pipe.js",
